	<profiles>
		<profile>
			<id>local</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<target-env>local</target-env>
			</properties>
		</profile>
		<profile>
			<id>dev</id>
			<properties>
				<target-env>dev</target-env>
			</properties>
		</profile>
		<profile>
			<id>qa18</id>
			<properties>
				<target-env>qa18</target-env>
			</properties>
		</profile>
		<profile>
			<id>hd</id>
			<properties>
				<target-env>hd</target-env>
			</properties>
		</profile>
		<profile>
			<id>prod</id>
			<properties>
				<target-env>prod</target-env>
			</properties>
		</profile>
		<profile>
			<id>omptest</id>
			<properties>
				<target-env>omptest</target-env>
			</properties>
