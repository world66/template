	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.0</version>
				<configuration>
					<source>${java.version}</source>
					<target>${java.version}</target>
					<encoding>UTF-8</encoding>
					<compilerArguments>
						<verbose />
						<bootclasspath>
							${java.home}/lib/rt.jar${path.separator}${java.home}/lib/jce.jar
						</bootclasspath>
					</compilerArguments>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<archive>
						<addMavenDescriptor>false</addMavenDescriptor>
					</archive>
					<webResources>
						<resource>
							<directory>src/main/filters/${target-env}</directory>
							<targetPath>WEB-INF/classes</targetPath>
							<excludes>
								<exclude>context.xml</exclude>
								<exclude>systemParam.properties</exclude>
							</excludes>
						</resource>
						<resource>
							<directory>src/main/filters/${target-env}</directory>
							<targetPath>META-INF</targetPath>
							<include>context.xml</include>
						</resource>
						<resource>
							<directory>src/main/filters/${target-env}</directory>
							<targetPath>config</targetPath>
							<include>systemParam.properties</include>
						</resource>
						<resource>
							<directory>src/main/filters/${target-env}</directory>
							<targetPath>WEB-INF/classes</targetPath>
							<include>redis.properties</include>
						</resource>
						<resource>
							<directory>src/main/filters/${target-env}</directory>
							<targetPath>WEB-INF/classes</targetPath>
							<include>messageConfig.properties</include>
						</resource>
					</webResources>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>

		<resources>
			<resource>
				<directory>src/main/resources/base</directory>
			</resource>
			<resource>
				<directory>src/main/resources/default</directory>
			</resource>
		</resources>

		<testResources>
			<testResource>
				<directory>src/test/resources/base</directory>
			</testResource>
		</testResources>

		<finalName>om</finalName>
	</build>

